<odoo>
    <data>

        <record model="ir.ui.view" id="tf_res_partner_tree_view">
            <field name="name">tf.res.partner tree</field>
            <field name="model">tf.res.partner</field>
            <field name="arch" type="xml">
                <tree string="Vendor Parameter">
                    <field name="name"/>
                    <field name="partner_id"/>
                    <field name="partner_type"/>
                    <field name="category_id"/>
                    <field name="purchase_discount"/>
                    <field name="partner_discount"/>
                    <field name="cost_deal_import"/>
                    <field name="fulfillment_rebate"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="tf_res_partner_form_view">
            <field name="name">itf.res.partner Form</field>
            <field name="model">tf.res.partner</field>
            <field name="arch" type="xml">
                <form string="Vendor Parameter">
                    <sheet>
                        <group>
                            <group id="header_left_group">
                                <field name="name"/>
                            </group>
                            <group id="header_left_group">
                                <field name="partner_id"/>
                            </group>
                            <group id="header_left_group">
                                <field name="partner_type"/>
                            </group>
                            <group id="header_left_group">
                                <field name="category_id"/>
                            </group>
                            <group id="header_left_group">
                                <field name="purchase_discount"/>
                            </group>
                            <group id="header_left_group">
                                <field name="partner_discount"/>
                            </group>
                            <group id="header_left_group">
                                <field name="cost_deal_import"/>
                            </group>
                            <group id="header_left_group">
                                <field name="fulfillment_rebate"/>
                            </group>
                        </group>
                    </sheet>

                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="tf_res_partner_action_window">
            <field name="name">Vendor Parameter</field>
            <field name="res_model">tf.res.partner</field>
            <field name="view_mode">tree,form</field>

        </record>

        <!-- <menuitem name="Vendor Parameter" id="menu_tf_res_partner_main"/> -->

        <menuitem name="Parametros de Proveedor" id="menu_tf_res_partner" action="tf_res_partner_action_window"
                  parent="sale.product_menu_catalog"/>

    </data>
</odoo>